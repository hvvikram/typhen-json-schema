"type": "object",

"properties": {
{{#each properties}}
  "{{name}}": {
  {{#if comment}}
    "description": "{{{escape comment}}}",
  {{/if}}
  {{#if tags}}
    {{#each tags}}
      {{{keyword ../type this}}}
    {{/each}}
  {{/if}}
  {{> type this }}
  }{{#unless @last}},{{/unless}}
{{/each}}
}

{{#unless additionalProperties}}
,"additionalProperties":
  {{#if stringIndex}}
    { {{> type type=stringIndex.type}} }
  {{else}}
    false
  {{/if}}
{{/unless}}

{{#if (required properties) }}
,"required": [{{{required properties}}}]
{{/if}}
